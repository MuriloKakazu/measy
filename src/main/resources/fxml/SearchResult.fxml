<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="self" styleClass="panel-dark-full" stylesheets="@../styles/measy/panel-dark-full.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.cefsa.ec6.measy.application.controller.ui.refactored.SearchResultController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label stylesheets="@../styles/measy/label.css" text="Tracks" />
            <FlowPane fx:id="tracksPanel" alignment="CENTER" prefWrapLength="${self.width}" />
            <Label stylesheets="@../styles/measy/label.css" text="Albums" />
            <FlowPane fx:id="albumsPanel" alignment="CENTER" prefWrapLength="${self.width}" />
            <Label stylesheets="@../styles/measy/label.css" text="Artists" />
            <FlowPane fx:id="artistsPanel" alignment="CENTER" prefWrapLength="${self.width}" />
            <Label stylesheets="@../styles/measy/label.css" text="Playlists" />
            <FlowPane fx:id="playlistsPanel" alignment="CENTER" prefWrapLength="${self.width}" />
         </children>
      </VBox>
   </children>
</AnchorPane>
